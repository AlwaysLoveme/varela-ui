{
  "name": "varela",
  "version": "1.0.0",
  "description": "a ui library of mobile for vuejs3.0",
  "main": "index.umd.js",
  "module": "index.esm.js",
  "typings": "index.d.ts",
  "source": "./example/main.js",
  "targets": {
    "index": {
      "includeNodeModules": {
        "vue": true
      },
      "context": "browser",
      "distDir": "./buildDir",
      "outputFormat": "esmodule",
      "scopeHoist": false,
      "optimize": true,
      "publicUrl": "/"
    }
  },
  "alias": {
    "@/*": "./src/"
  },
  "scripts": {
    "serve": "rimraf .parcel-cache && rimraf dist && parcel serve ./example/index.html --host=0.0.0.0",
    "clean": "rimraf ./lib",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.vue",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && cpy ./package.json ./lib && npm run lint && rollup --config build/rollup.config.prod.js --watch"
  },
  "keywords": [
    "vuejs",
    "ui library",
    "typescript"
  ],
  "author": "zhuxian",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "ssr-window": "^3.0.0",
    "vconsole": "^3.7.0",
    "vue": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@material/button": "^11.0.0",
    "@material/ripple": "^11.0.0",
    "@parcel/babylon-walk": "^2.0.0-beta.3.1",
    "@parcel/transformer-babel": "^2.0.0-beta.3.1",
    "@parcel/transformer-image": "^2.0.0-beta.3.1",
    "@parcel/transformer-sass": "^2.0.0-beta.3.1",
    "@parcel/transformer-vue": "^2.0.0-beta.3.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/hammerjs": "^2.0.39",
    "@types/qs": "^6.9.6",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.2.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "cpy-cli": "^3.1.1",
    "cssnano": "^4.1.11",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "parcel": "^2.0.0-beta.3.1",
    "prettier": "^2.3.1",
    "rimraf": "^2.7.1",
    "rollup": "^2.51.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.34.1",
    "typescript": "^4.3.2"
  }
}
